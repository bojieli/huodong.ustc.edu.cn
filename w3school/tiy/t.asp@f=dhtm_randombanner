len=3

now=new Date()
now=now.getSeconds()
rnd=now%len

image=eval("image"+rnd)
href=eval("href"+rnd)
alt=eval("alt"+rnd)

document.write("<a href='http://www.w3school.com.cn/tiy/&quot;&#32;+&#32;href&#32;+&#32;&quot;'>")
document.write("<img src='http://www.w3school.com.cn/tiy/&quot;&#32;+&#32;image&#32;+&#32;&quot;' alt='" + alt + "'></a>")
</script>

